FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "10/bottomWater";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1000;

boundaryField
{
    minX
    {
        type            calculated;
        value           uniform 1000;
    }
    maxX
    {
        type            calculated;
        value           uniform 1000;
    }
    minY
    {
        type            calculated;
        value           uniform 1000;
    }
    minZ
    {
        type            calculated;
        value           uniform 1000;
    }
    maxZ
    {
        type            calculated;
        value           uniform 1000;
    }
    bottomWater_to_rightSolid
    {
        type            calculated;
        value           uniform 1000;
    }
    bottomWater_to_leftSolid
    {
        type            calculated;
        value           uniform 1000;
    }
    bottomWater_to_heater
    {
        type            calculated;
        value           uniform 1000;
    }
}


